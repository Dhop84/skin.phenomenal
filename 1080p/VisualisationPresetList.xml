<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>5</defaultcontrol>
	<coordinates>
		<posx>502</posx>
		<posy>100</posy>
	</coordinates>
	
	<controls>
	<control type="image">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>900</width>
			<height>700</height>
			<texture background="true" colordiffuse="grey_50">white.png</texture>
			<bordertexture colordiffuse="grey">white.png</bordertexture>
			<bordersize>2</bordersize>
			<animation effect="fade" end="60" time="0">WindowOpen</animation>
		</control>
		<control type="label" id="2">
			<description>heading label</description>
			<posx>60</posx>
			<posy>10</posy>
			<width>765</width>
			<height>45</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>font-30</font>
			<label>-</label>
			<textcolor>white</textcolor>
		</control>
		<control type="label" id="4">
			<description>No Presets at all label</description>
			<posx>60</posx>
			<posy>300</posy>
			<width>765</width>
			<height>45</height>
			<label>13389</label>
			<align>center</align>
			<font>font-23</font>
			<textcolor>white</textcolor>
		</control>
		<control type="list" id="5">
			<posx>68</posx>
			<posy>70</posy>
			<width>750</width>
			<height>600</height>
			<onup>5</onup>
			<ondown>5</ondown>
			<onleft>5</onleft>
			<onright>61</onright>
			<pagecontrol>61</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout width="750" height="44">
				<control type="label">
					<posx>15</posx>
					<posy>0</posy>
					<width>720</width>
					<height>45</height>
					<font>font-20</font>
					<align>right</align>
					<aligny>center</aligny>
					<textcolor>lightgrey</textcolor>
					<selectedcolor>$VAR[main_color]</selectedcolor>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</itemlayout>
			<focusedlayout width="750" height="44">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>750</width>
					<height>45</height>
					<texture colordiffuse="grey_b" background="true">white.png</texture>
					<animation effect="fade" start="100" end="50" time="100" condition="!Control.HasFocus(3)">Conditional</animation>
				</control>
				<control type="label">
					<posx>15</posx>
					<posy>0</posy>
					<width>720</width>
					<height>45</height>
					<font>font-20</font>
					<align>right</align>
					<aligny>center</aligny>
					<textcolor>grey_50</textcolor>
					<selectedcolor>$VAR[main_color]</selectedcolor>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</focusedlayout>
		</control>
		
		<control type="scrollbar" id="61">
			<posx>840</posx>
			<posy>75</posy>
			<width>10</width>
			<height>600</height>
			<include>scroll_bar</include>
			<onleft>5</onleft>
			<onright>5</onright>
			<ondown>5</ondown>
			<onup>5</onup>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
			<visible>Control.IsVisible(5)</visible>
		</control>
	</controls>
</window>
