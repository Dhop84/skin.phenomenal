<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">11</defaultcontrol>
	<coordinates>
		<posx>502</posx>
		<posy>300</posy>
	</coordinates>
	<animation effect="zoom" start="0" end="100" center="900,480" time="200">WindowOpen</animation>
	<animation effect="zoom" start="100" end="0" center="900,480" time="200">WindowClose</animation>
	<controls>
		<control type="image">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>900</width>
			<height>390</height>
			<texture background="true" colordiffuse="grey_50">white.png</texture>
			<bordertexture colordiffuse="grey">white.png</bordertexture>
			<bordersize>2</bordersize>
		</control>

		<control type="label" id="1">
			<description>heading label</description>
			<posx>60</posx>
			<posy>10</posy>
			<width>765</width>
			<height>45</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>font-30</font>
			<label>-</label>
			<textcolor>white</textcolor>
		</control>
		<control type="image">
			<posx>0</posx>
			<posy>60</posy>
			<width>900</width>
			<height>3</height>
			<texture background="true">white.png</texture>
		</control>
		<control type="textbox" id="9">
			<posx>15</posx>
			<posy>90</posy>
			<width>870</width>
			<height>205</height>
			<align>center</align>
			<label>-</label>
			<font>font-22</font>
			<autoscroll time="2000" delay="8000" repeat="5000">!Skin.HasSetting(AutoScroll)</autoscroll>
		</control>
		<control type="grouplist" id="12">
			<posx>140</posx>
			<posy>322</posy>
			<width>640</width>
			<height>60</height>
			<itemgap>20</itemgap>
			<orientation>horizontal</orientation>
			<onleft>12</onleft>
			<onright>12</onright>
			<onup>12</onup>
			<ondown>12</ondown>
			<control type="button" id="11">
				<description>Yes button</description>
				<width>300</width>
				<height>52</height>
				<label>107</label>
				<font>font-23</font>
				<include>button_dialogs</include>
				<align>center</align>
			</control>
			<control type="button" id="10">
				<description>No button</description>
				<width>300</width>
				<height>52</height>
				<label>106</label>
				<font>font-23</font>
				<include>button_dialogs</include>
				<align>center</align>
			</control>
		</control>
	</controls>
</window>
